<body>
    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">
        <div class="d-flex align-items-center justify-content-between">
            <a href="index.html" class="logo d-flex align-items-center">
                <img src="/img/logo.png" alt="" />
                <span class="d-none d-lg-block">NiceAdmin</span>
            </a>
        </div>
        <!-- End Logo -->

        <div class="search-bar">
            <form class="search-form d-flex align-items-center" method="POST" action="/home/profil">
                <input type="text" name="query" placeholder="Search" title="Enter search keyword" />
                <button type="submit" title="Search">
                    <i class="bi bi-search"></i>
                </button>
            </form>
        </div>
        <!-- End Search Bar -->

        <nav class="header-nav ms-auto">
            <ul class="d-flex align-items-center">
                <li class="nav-item d-block d-lg-none">
                    <a class="nav-link nav-icon search-bar-toggle" href="#">
                        <i class="bi bi-search"></i>
                    </a>
                </li>
                <!-- End Search Icon-->



                <li class="nav-item dropdown pe-3">
                    <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
                        <img src="/img/profile-img.jpg" alt="Profile" class="rounded-circle" />
                        <span class="d-none d-md-block ps-2">
                            <%= user.nama %>
                        </span> </a><!-- End Profile Iamge Icon -->


                    <!-- End Profile Dropdown Items -->
                </li>
                <!-- End Profile Nav -->
            </ul>
        </nav>
        <!-- End Icons Navigation -->
    </header>
    <!-- End Header -->



    <!-- Profile Edit Form -->
    <form>

        <div class="row mb-3">
            <label for="profileImage" class="col-md-4 col-lg-3 col-form-label">Profile Image</label>
            <div style="margin: 70px 0px 20px 600px
          ;" class="col-md-8 col-lg-9">
            </div>
        </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="fullName" class="col-md-1 col-lg-1 col-form-label">Full Name</label>
            <div class="col-md-8 col-lg-9">
                <input name="nama" type="text" class="form-control" id="fullName" value="<%= user.nama%>" />
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="about" class="col-md-1 col-lg-1 col-form-label">Gender</label>
            <div class="col-md-8 col-lg-9">
                <textarea name="gender" class="form-control" id="about"
                    style="height: 100px"><%= user.gender %></textarea>
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="Address" class="col-md-1 col-lg-1 col-form-label">Address</label>
            <div class="col-md-8 col-lg-9">
                <input name="address" type="text" class="form-control" id="Address" value="<%= user.address %>" />
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="Phone" class="col-md-1 col-lg-1 col-form-label">Contact</label>
            <div class="col-md-8 col-lg-9">
                <input name="contact" type="text" class="form-control" id="Phone" value="<%= user.contact_number %>" />
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="Email" class="col-md-1 col-lg-1 col-form-label">Email</label>
            <div class="col-md-8 col-lg-9">
                <input name="email" type="email" class="form-control" id="Email" value="<%= user.email %>" />
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="Twitter" class="col-md-1 col-lg-1 col-form-label">Experience</label>
            <div class="col-md-8 col-lg-9">
                <input name="experience" type="text" class="form-control" id="Twitter" value="<%= user.experience%>" />
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="Facebook" class="col-md-1 col-lg-1 col-form-label">Year of Experience</label>
            <div class="col-md-8 col-lg-9">
                <input name="exp" type="text" class="form-control" id="Facebook" value="<%= user.exp %>" />
            </div>
        </div>

        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
            <label for="Instagram" class="col-md-1 col-lg-1 col-form-label">Education</label>
            <div class="col-md-8 col-lg-9">
                <input name="education" type="text" class="form-control" id="Instagram" value="<%= user.education %>" />
            </div>
        </div>
        <!--
        <div style="margin: 50px 0px 0px 50px;" class="row mb-3">
          <label
            for="Linkedin"
            class="col-md-1 col-lg-1 col-form-label"
            >Linkedin Profile</label
          >
          <div class="col-md-8 col-lg-9">
            <input
              name="linkedin"
              type="text"
              class="form-control"
              id="Linkedin"
              value="https://linkedin.com/#"
            />
          </div>
        </div> -->

        <div style="margin: 50px 0px 50px 0px;" class="text-center">
            <button type="submit" class="btn btn-primary">
                Save Changes
            </button>
        </div>
    </form>
    <!-- End Profile Edit Form -->
    </div>

